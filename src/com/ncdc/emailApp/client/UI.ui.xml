<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	</ui:style>
	<g:HTMLPanel>
	<h2>UI Binder</h2>
	<form style="padding: 10px; background: #eee; border; 3px sp;od #aaa; margin 50px">
		<table>
			<tr>
				<g:TextBox ui:field='nameBox'/>
			</tr>
			<tr>
				<g:TextBox ui:field='surnameBox'/>
			</tr>
			<tr>
				<g:TextBox ui:field='phoneBox'/>
			</tr>
			<tr>
				<g:TextBox ui:field='emailBox'/>
			</tr>
			<tr>
				<g:RadioButton ui:field="radioButton1" name="radio">
					Subscribe to newsletter
				</g:RadioButton>
				<g:RadioButton ui:field="radioButton2" name="radio">
					Don't subscribe to newsletter
				</g:RadioButton>
			</tr>
			<tr>
				<g:Button ui:field="button">Send email</g:Button>
			</tr>
		</table>
	</form>
	</g:HTMLPanel>
</ui:UiBinder> 